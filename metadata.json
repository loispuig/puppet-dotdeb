	"requirements": [
		{"name": "puppet", "version_requirement": ">= 3.7.0"}
	],
	"dependencies": [
		{"name":"puppetlabs/stdlib","version_requirement":">= 4.5.0 < 5.0.0"},
		{"name":"puppetlabs/apt","version_requirement":">= 2.2.2"}
	]
}