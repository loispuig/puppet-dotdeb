{
	"name": "dotdeb",
	"version": "0.1.0",
	"author": "loispuig",
	"summary": "Setup dotdeb repository on Debian OS",
	"license": "MIT",
	"source": "https://github.com/loispuig/puppet-dotdeb",
	"project_page": "https://github.com/loispuig/puppet-dotdeb",
	"issues_url": "https://github.com/loispuig/puppet-dotdeb/issues",
	"operatingsystem_support": [
		{
			"operatingsystem": "Debian",
			"operatingsystemrelease": [ "6", "7", "8" ]
